{
  "expo": {
    "name": "hackathon-frontend",
    "slug": "hackathon-frontend",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "hackathonfrontend",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.anonymous.hackathonfrontend"
    },
    "web": {
      "output": "static"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      [
        "expo-font",
        {
          "fonts": [
            "./assets/fonts/AvenirLTProBook.otf",
            "./assets/fonts/AvenirLTProHeavy.otf",
            "./assets/fonts/AvenirLTProMedium.otf",
            "./assets/fonts/AvenirLTProLight.otf",
            "./assets/fonts/AvenirLTProRoman.otf"
          ]
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    }
  }
}
